#!/usr/bin/env bash
set -eo pipefail

echo "Detect script starting"

# 1. GET ARGS
plan_path=$2

# 2. DECLARE DEPENDENCIES (OPTIONAL)
cat >> "${plan_path}" <<EOL
# Buildpack provides this dependency
#
# NOTE: The dependency is provided during the 'build' process.
#
[[provides]]
name = "dyno-metrics"
EOL

echo "Detect script complete"
exit 0
